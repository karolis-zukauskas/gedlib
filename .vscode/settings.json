{
  "git.ignoreLimitWarning": true,
  "files.associations": {
    "*.json": "jsonc",
    "*.ipp": "cpp"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.html": true,
    "build": true,
    "lib": true,
    "bin": true,
    "docs": true,
    "out": true
  }

}